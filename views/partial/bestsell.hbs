<div class="row">
    <div class="col-xl-12">
        <div class="product-tab-content">
            <div class="tab-content" id="myTabContent1">
                <div class="tab-pane fade show active" id="home1" role="tabpanel" aria-labelledby="home-tab">
                    <div class="product-slider owl-carousel">
                        {{#each firstPartProducts}}
                        <div class="pro-item">
                            <div class="product-wrapper mb-50">
                                <div class="product-img mb-25">
                                    <a href="/productinfo?q={{this._id}}">
                                        <img src="{{this.imageUrls.[0]}}" alt="">
                                        <img class="secondary-img" src={{this.imageUrls.[2]}} alt="">
                                    </a>
                                    <div class="product-action text-center">
                                        <a href="#" title="Shopping Cart">
                                            <i class="flaticon-shopping-cart"></i>
                                        </a>
                                        <a href="#" title="Quick View">
                                            <i class="flaticon-eye"></i>
                                        </a>
                                        <a href="#" data-toggle="tooltip" data-placement="right" title="Compare">
                                            <i class="flaticon-compare"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="product-content">
                                    <div class="pro-cat mb-10">
                                        <a href="shop.html">decor, </a>
                                        <a href="shop.html">furniture</a>
                                    </div>
                                    <h4>
                                        <a href="product-details.html">{{this.name}}</a>
                                    </h4>
                                    <div class="product-meta">
                                        <div class="pro-price">
                                            <span>{{this.price}}</span>
                                            <span class="old-price">{{this.oldprice}}</span>
                                        </div>
                                    </div>
                                    <div class="product-wishlist">
                                        <a href="#"><i class="far fa-heart" title="Wishlist"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/each}}

                        <!-- Insert "Best Sell Products" section here -->
                        <section class="product-area box-90 pt-45 pb-40">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-xl-5 col-lg-12">
                                        <div class="area-title mb-50">
                                            <h2>Best Sell Products</h2>
                                            <p>Browse the huge variety of our products</p>
                                        </div>
                                    </div>
                                    <div class="col-xl-7 col-lg-12">
                                        <div class="product-tab mb-40">
                                            <ul class="nav product-nav justify-content-xl-end" id="myTab" role="tablist">
                                                <!-- Add your tab items here -->
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-12">
                                        <div class="product-tab-content">
                                            <div class="tab-content" id="myTabContent1">
                                                <!-- Add your tab content here -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>

                        {{#each remainingProducts}}
                        <div class="pro-item">
                            <div class="product-wrapper mb-50">
                                <div class="product-img mb-25">
                                    <a href="/productinfo?q={{this._id}}">
                <img src="{{this.imageUrls.[0]}}" alt="">
                                        <img class="secondary-img" src={{this.imageUrls.[2]}} alt="">
                                    </a>
                                    <div class="product-action text-center">
                                        <a href="#" title="Shopping Cart">
                                            <i class="flaticon-shopping-cart"></i>
                                        </a>
                                        <a href="#" title="Quick View">
                                            <i class="flaticon-eye"></i>
                                        </a>
                                        <a href="#" data-toggle="tooltip" data-placement="right" title="Compare">
                                            <i class="flaticon-compare"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="product-content">
                                    <div class="pro-cat mb-10">
                                        <a href="shop.html">decor, </a>
                                        <a href="shop.html">furniture</a>
                                    </div>
                                    <h4>
                                        <a href="product-details.html">{{this.name}}</a>
                                    </h4>
                                    <div class="product-meta">
                                        <div class="pro-price">
                                            <span>{{this.price}}</span>
                                            <span class="old-price">{{this.oldprice}}</span>
                                        </div>
                                    </div>
                                    <div class="product-wishlist">
                                        <a href="#"><i class="far fa-heart" title="Wishlist"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
